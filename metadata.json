{
  "name": "Lavaburn-razor",
  "version": "0.4.1",
  "author": "Nicolas Truyens (Lavaburn)",
  "license": "Apache-2.0",
  "summary": "Puppet module for managing (Puppetlabs) Razor",
  "source": "https://github.com/Lavaburn/puppet-razor",
  "project_page": "https://github.com/Lavaburn/puppet-razor",
  "issues_url": "https://github.com/Lavaburn/puppet-razor/issues",
  "tags": [
    "razor",
    "provisioning"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6.5",
        "7.2"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.0.0 <5.0.0"
    },
    {
      "name": "reidmv/yamlfile",
      "version_requirement": ">=0.2.0 < 1.0.0"
    }
  ]
}
